
<div class="box-body">
    <div class="table-responsive">
        <table id="data-table" class="table table-striped">
            <thead>
                <tr>
                    <th>No.</th>
                    <th>Full Name</th>
                    <th>Products Into Account</th>
                    <th>State</th>
                    <th>Country</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="datum in userData">
                    <td>{{datum.no}}</td>
                    <td><a href="/Public/User/{{datum.subId}}">{{datum.fullName}}</a></td>
                    <td>{{datum.productsIntoAccount}}</td>
                    <td>{{datum.state}}</td>
                    <td>{{datum.country}}</td>
                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <th>No.</th>
                    <th>Full Name</th>
                    <th>Products Into Account</th>
                    <th>State</th>
                    <th>Country</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>