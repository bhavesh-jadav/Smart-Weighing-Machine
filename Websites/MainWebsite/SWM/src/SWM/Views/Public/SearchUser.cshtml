@model List<SearchUserModel>
@section css{
    <link href="https://cdn.datatables.net/1.10.13/css/dataTables.bootstrap.min.css" rel="stylesheet" />
}
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">
                        Found @ViewBag.SearchResultLength result(s).
                        @if (ViewBag.SearchResultLength > 0)
                        {
                             <span>Click on full name for more details.</span>
                        }
                    </h3>
                </div>
                <div class="box-body">
                    @Html.Partial("ShowUserPublicPartial", Model)
                </div>
            </div>
        </div>
    </div>
</section>
@section scripts{
    <script src="https://cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.13/js/dataTables.bootstrap.min.js"></script>
    <script>
      $(function () {
          $("#data-table").DataTable({
              "responsive": true,
              "iDisplayLength": 25
          });
      });
    </script>
}