<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>@ViewBag.Title - SWM</title>
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/admin-lte/2.3.11/css/AdminLTE.min.css">
        <link href="~/distribution/css/site.css" rel="stylesheet" />
        @RenderSection("css", false)
    </head>
<body class="hold-transition login-page" style="padding:1px;">
    <div class="login-box">
        <div class="login-logo">
            <a asp-controller="Home" asp-action="Index"><b>SWM</b></a>
        </div>
        @RenderBody()
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    @RenderSection("scripts", false)
    <footer style="position: absolute;bottom: auto;text-align: center;width: 100%;margin-left: auto;margin-right: auto;">
        <strong>Made by <a href="https://www.linkedin.com/in/bhavesh-jadav" target="_blank">Bhavesh Jadav</a> &copy; 2016-2017. Front end by <a href="http://almsaeedstudio.com">Almsaeed Studio</a> &copy; 2014-2016.</strong> All rights
        reserved.
    </footer>
</body>
</html>
